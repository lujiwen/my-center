            <Label Canvas.Left="299" Canvas.Top="197" Content="L/min" Style="{StaticResource unitLableStyle}" Width="55"/>
            <Label Canvas.Left="293" Canvas.Top="95" Content="L/min" Style="{StaticResource unitLableStyle}" Width="55"/>
            <Label Canvas.Left="292" Canvas.Top="33" Content="L/min" Style="{StaticResource unitLableStyle}" Width="55"/>
