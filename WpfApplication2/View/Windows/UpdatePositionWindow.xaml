<Window x:Class="WpfApplication2.View.Windows.UpdatePositionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
          Title="添加用户" Height="650" Width="550" Background="#FF565656" Foreground="White" Topmost="true" WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel Orientation="Vertical" Margin="10">
            <StackPanel Name="building_position_panel" Orientation="Vertical">
            </StackPanel>
            <StackPanel Margin="10,0,10,10"  HorizontalAlignment="Center" Orientation="Horizontal">
                <Button Margin="10"   Content="取消" Height="23" Name="cancel" Width="75" Click="cancel_Click" />
                <Button  Margin="10"   Content="确定" Height="23" Name="ok" Width="75" Click="ok_Click" />
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
